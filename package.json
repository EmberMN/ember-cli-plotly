{
  "name": "ember-cli-plotly",
  "version": "0.7.0",
  "description": "A basic charting component using Plotly.js",
  "keywords": [
    "ember-addon",
    "data-visualization",
    "visualization",
    "chart",
    "charting",
    "graph",
    "graphing",
    "plot",
    "plotting",
    "plotly",
    "plotly.js"
  ],
  "repository": "https://github.com/EmberMN/ember-cli-plotly",
  "license": "MIT",
  "author": "Jacob Quant <jacobq@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@ember/test-waiters": "^3.0.1",
    "debug": "4.3.4",
    "ember-auto-import": "^2.4.1",
    "ember-cli-babel": "7.26.11",
    "ember-cli-htmlbars": "6.0.1",
    "ember-decorators": "6.1.1",
    "ember-lodash": "4.19.5",
    "plotly.js": "2.11.1"
  },
  "devDependencies": {
    "@babel/core": "^7",
    "@ember/optional-features": "2.0.0",
    "@ember/test-helpers": "^2.7.0",
    "@embroider/test-setup": "^0.45.0",
    "@glimmer/component": "^1.1.1",
    "@glimmer/tracking": "^1.1.1",
    "babel-eslint": "10.1.0",
    "broccoli-asset-rev": "3.0.0",
    "ember-cli": "3.28.0",
    "ember-cli-dependency-checker": "3.2.0",
    "ember-cli-deprecation-workflow": "2.1.0",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-sri": "2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-cli-testdouble": "0.1.4",
    "ember-component-css": "0.7.4",
    "ember-decorators-polyfill": "1.1.5",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-export-application-global": "2.0.1",
    "ember-load-initializers": "2.1.2",
    "ember-maybe-import-regenerator": "1.0.0",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "5.1.5",
    "ember-resolver": "8.0.3",
    "ember-source": "3.28.1",
    "ember-source-channel-url": "3.0.0",
    "ember-template-lint": "^3.8.0",
    "ember-truth-helpers": "3.0.0",
    "ember-try": "1.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "10.5.9",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^6.2.0",
    "loader.js": "4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "qunit": "^2.18.1",
    "qunit-dom": "2.0.0",
    "safe-json-stringify": "1.2.0",
    "testdouble": "3.16.4",
    "webpack": "5"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
